ifndef _included_targets_mk
_included_targets_mk = yes

# targets.mk - Defines basic versions of the targets all Makefiles provide
#------------------------------------------------------------------------------
#
# Usage:
#
#    Every Makefile should includes this fragment, directly or indirectly,
#    before any actual do-something fragments.
#
# Requires:
#
#    Nothing
#
# Provides:
#
#    Do-nothing targets for external targets "all, clean, distclean,
#      reportObjs"
#
#    Do-nothing targets for internal targets "_cleanHere, _distcleanHere"
#
#    Initialization of RECURSIVE_PATH to "." if it's undefined.

MAKE_PARALLELISM = @MAKE_PARALLELISM@

MAKEFLAGS += @NOPRINTDIR@

ifeq ($(RECURSIVE_PATH),)
  RECURSIVE_PATH=.
endif

all::
	@echo Making all in $(RECURSIVE_PATH)

clean::
	@echo Making clean in $(RECURSIVE_PATH)
	@$(MAKE) _cleanHere

distclean::
	@echo Making distclean in $(RECURSIVE_PATH)
	@$(MAKE) _cleanHere _distcleanHere

reportObjs::
	@true

_cleanHere::
	@true

_distcleanHere::
	rm -f Makefile

.PRECIOUS: Makefile

Makefile: $(SRCDIR)/Makefile.in ${OBJTOP}/config.status
	@( Here="`pwd`" ; cd $(OBJTOP) ; Top="`pwd`" ; CONFIG_FILES="`echo $${Here} | sed -e "s!^$${Top}/!!" -e "s!^$${Top}!.!"`/Makefile" CONFIG_HEADERS="" ./config.status ) 1>&2

endif
