# SCI Network 1.0

if {[llength [array get env SCIRUN_DATA]] == 0} {
   puts ""
   puts "*** Error: this net depends on the SCIRUN_DATA enviroment variable."
   puts "*** Please exit, set it in your shell, and rerun."
   puts "*** (e.g. setenv SCIRUN_DATA /local/data/SCIRunData/)"
   puts ""
   ::netedit scheduleok
   return
}
set DATADIR $env(SCIRUN_DATA)
puts "*** Using SCIRUN_DATA $DATADIR"

::netedit dontschedule

global notes
set notes ""

set m0 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 204 297]
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 144 107]
set m2 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 308 202]
set m3 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 308 32]
set m4 [addModuleAtPosition "SCIRun" "Render" "Viewer" 368 400]
set m5 [addModuleAtPosition "SCIRun" "Fields" "SamplePlane" 560 191]
set m6 [addModuleAtPosition "SCIRun" "Fields" "DirectInterpolate" 493 321]

addConnection $m3 0 $m2 0
addConnection $m2 0 $m0 1
addConnection $m0 1 $m4 0
addConnection $m1 0 $m5 0
addConnection $m5 0 $m6 1
addConnection $m1 0 $m6 0
addConnection $m6 0 $m2 1
addConnection $m6 0 $m0 0

set $m0-notes {}
set $m0-show_status {1}
set $m0-isoval {0.4260}
