# SCI Network 1.0

if {[llength [array get env SCIRUN_DATA]] == 0} {
   puts ""
   puts "*** Error: this net depends on the SCIRUN_DATA enviroment variable."
   puts "*** Please exit, set it in your shell, and rerun."
   puts "*** (e.g. setenv SCIRUN_DATA /local/data/SCIRunData/)"
   puts ""
   ::netedit scheduleok
   return
}
set DATADIR $env(SCIRUN_DATA)
puts "*** Using SCIRUN_DATA $DATADIR"

if {[llength [array get env SCIRUN_DATASET]] == 0} {
   puts ""
   puts "*** Environment variable 'SCIRUN_DATASET' was not set -- "
   puts "*** using 'sphere' as the default."
   puts ""
   set DATASET sphere
} else {
   set DATASET $env(SCIRUN_DATASET)
   puts "*** Using DATASET $DATASET"
}

source $DATADIR/$DATASET/$DATASET-settings.net

::netedit dontschedule

global userName
set userName "dmw"

global runDate
set runDate " Thu  Apr 19 2001"

global runTime
set runTime " 22:8:0"

set m0 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 304 10]
set m1 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 92 114]
set m2 [addModuleAtPosition "SCIRun" "Render" "Viewer" 110 493]
set m3 [addModuleAtPosition "Teem" "Filters" "NrrdConvert" 8 216]
set m4 [addModuleAtPosition "Teem" "Filters" "NrrdPermute" 156 215]
set m5 [addModuleAtPosition "Teem" "Filters" "NrrdQuantize" 304 216]
set m6 [addModuleAtPosition "Teem" "Filters" "NrrdResample" 451 216]
set m7 [addModuleAtPosition "Teem" "Filters" "NrrdSubvolume" 599 216]
set m8 [addModuleAtPosition "Teem" "DataIO" "FieldToNrrd" 304 111]
set m9 [addModuleAtPosition "Teem" "DataIO" "NrrdToField" 304 312]
set m10 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 304 399]

addConnection $m1 1 $m2 0
addConnection $m0 0 $m8 0
addConnection $m9 0 $m10 0
addConnection $m10 1 $m2 1
addConnection $m0 0 $m1 0
addConnection $m8 0 $m6 0
addConnection $m6 0 $m9 0

set $m0-filename $DATADIR/$DATASET/$DATASET-mri-16.lvd.fld
set $m1-isoval 28.35
set $m1-extract-from-new-field 1
set $m1-algorithm 0
set $m1-build_trisurf 0
set $m1-np 1
$m2 ui
set $m2-ViewWindow_0-view-eyep-x ${view-eyep-x}
set $m2-ViewWindow_0-view-eyep-y ${view-eyep-y}
set $m2-ViewWindow_0-view-eyep-z ${view-eyep-z}
set $m2-ViewWindow_0-view-lookat-x ${view-lookat-x}
set $m2-ViewWindow_0-view-lookat-y ${view-lookat-y}
set $m2-ViewWindow_0-view-lookat-z ${view-lookat-z}
set $m2-ViewWindow_0-view-up-x ${view-up-x}
set $m2-ViewWindow_0-view-up-y ${view-up-y}
set $m2-ViewWindow_0-view-up-z ${view-up-z}
set $m2-ViewWindow_0-view-fov ${view-fov}
set $m3-type 10
set $m4-axis0 0
set $m4-axis1 1
set $m4-axis2 2
set $m5-minf 0
set $m5-maxf 255
set $m5-nbits 16
set $m6-filtertype {tent}
set $m6-resampAxis0 {20}
set $m6-resampAxis1 {20}
set $m6-resampAxis2 {20}
set $m7-minAxis0 {0}
set $m7-maxAxis0 {20}
set $m7-minAxis1 {0}
set $m7-maxAxis1 {20}
set $m7-minAxis2 {0}
set $m7-maxAxis2 {20}
set $m10-isoval 27.5
set $m10-extract-from-new-field 1
set $m10-algorithm 0
set $m10-build_trisurf 0
set $m10-np 1
$m6 ui

::netedit scheduleok
