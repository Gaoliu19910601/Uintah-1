
#include <config/sci.cf>

#ifdef SCI_MACHINE_Linux
TEMPLATE_OBJS = templates.o
TEMPLATE_SRCS = templates.cc
#endif

OBJS = Tracker.o  TrackerServer.o  \
	$(TEMPLATE_OBJS)
SRCS = Tracker.cc TrackerServer.cc \
	$(TEMPLATE_SRCS)

LIBNAME = Devices

SO_LIBS =
SO_LDFLAGS = -L$(TOP)/lib -lComm -lClasslib -lMultitask -lMalloc SCI_CCLibs

SCIDependTarget($(SRCS))
SCIObjLibraryTarget($(LIBNAME),$(OBJS),$(SO_LIBS), $(SO_LDFLAGS))
