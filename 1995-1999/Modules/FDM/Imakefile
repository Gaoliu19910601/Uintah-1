
#include <config/sci.cf>

#ifdef SCI_MACHINE_Linux
TEMPLATE_OBJS = templates.o
TEMPLATE_SRCS = templates.cc
#endif

OBJS = BuildFDField.o  BuildFDMatrix.o \
	$(TEMPLATE_OBJS)
SRCS = BuildFDField.cc BuildFDMatrix.cc \
	$(TEMPLATE_SRCS)

LIBNAME = FDM

SO_LIBS =
SO_LDFLAGS = -L$(TOP)/lib -lDatatypes -lDataflow -lClasslib \
	-lMultitask -lMalloc

SCIDependTarget($(SRCS))
SCIObjLibraryTarget($(LIBNAME),$(OBJS),$(SO_LIBS),$(SO_LDFLAGS))
