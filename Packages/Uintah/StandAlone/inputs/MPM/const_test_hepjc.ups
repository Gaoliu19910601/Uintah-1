<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->
<!-- @version: Updated 7/31/00-->
<Uintah_specification>

   <Meta>
	<title>Constitutive Model Tester</title>
   </Meta>

   <Time>
	<maxTime>2</maxTime>
	<initTime>0.0</initTime>
	<delt_min>0.000001</delt_min>
	<delt_max>0.00001</delt_max>
	<timestep_multiplier>0.8</timestep_multiplier>
   </Time>
   <DataArchiver>
        <filebase>const_test_hepjc.uda</filebase>
	<outputInterval>.001</outputInterval>
<!--
        <save label = "NTractionZMinus"/>
	<save label = "KineticEnergy"/>
	<save label = "TotalMass"/>
	<save label = "StrainEnergy"/>
	<save label = "CenterOfMassPosition"/>
	<save label = "CenterOfMassVelocity"/>
-->
	<save label = "p.x"/>
	<save label = "p.velocity"/>
	<save label = "p.mass"/>
	<save label = "p.volume"/>
	<save label = "p.stress"/>
	<save label = "p.deformationMeasure"/>
	<save label = "p.plasticStrain"/>
	<save label = "p.damage"/>
<!--
	<save label = "g.mass"/>
	<save label = "g.stressFS"/>
-->
	<checkpoint cycle = "2" interval = "0.0029"/>
   </DataArchiver>

    <MPM>
	<time_integrator>explicit</time_integrator>
        <nodes8or27>8</nodes8or27>
    </MPM>

    <PhysicalConstants>
	<gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
	<MPM>
	    <material>
                <burn type = "null" />
		<density>7830.0</density>
		<constitutive_model type="hypoelastic_plastic">
		   <shear_modulus>80.0e9</shear_modulus>
		   <bulk_modulus>173.33e9</bulk_modulus>
                   <tolerance>1.0e-10</tolerance>
                   <plasticity_model type="johnson_cook">
                     <A>792.0e6</A>
                     <B>510.0e6</B>
                     <C>0.014</C>
                     <n>0.26</n>
                     <m>1.03</m>
	           </plasticity_model>
                   <damage_model type="johnson_cook">
                     <D1>0.05</D1>
                     <D2>3.44</D2>
                     <D3>-2.12</D3>
                     <D4>0.002</D4>
                     <D5>0.61</D5>
                   </damage_model>
	        </constitutive_model>
		<toughness>10.e6</toughness>
		<thermal_conductivity>1.0</thermal_conductivity>
		<specific_heat>477</specific_heat>
                <room_temp>294.0</room_temp>
                <melt_temp>1793.0</melt_temp>
		<geom_object>
		    <box label = "box - 4340 Steel">
			<min>[1.0,1.0,1.0]</min>
			<max>[2.0,2.0,2.0]</max>
		    </box>
		    <res>[1,1,1]</res>
		    <velocity>[0.0,0.0,570.0]</velocity>
		    <temperature>294</temperature>
 		</geom_object>
		<velocity_field>0</velocity_field>
	    </material>

	    <contact>
	      <type>rigid</type>
	      <vel_fields>[0,0,0]</vel_fields>
              <stop_time>999999.9 </stop_time>
	    </contact>
	</MPM>

    </MaterialProperties>
	
    <Grid>
	<BoundaryConditions>
	   <Face side = "x-">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>
           <Face side = "x+">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>
           <Face side = "y-">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>                  
	   <Face side = "y+">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>                  
           <Face side = "z-">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>                  
	   <Face side = "z+">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>         	 
	</BoundaryConditions>
	<Level>
	    <Box label = "1">
		<lower>[0.0,0.0,0.0]</lower>
		<upper>[3.0,3.0,3.0]</upper>
	        <resolution>[3,3,3]</resolution>
		<extraCells>[1,1,1]</extraCells>
		<patches>[1,1,1]</patches>
	    </Box>
	</Level>
    </Grid>

    <PhysicalBC>
	<MPM>
	</MPM>
    </PhysicalBC>
    
</Uintah_specification>

