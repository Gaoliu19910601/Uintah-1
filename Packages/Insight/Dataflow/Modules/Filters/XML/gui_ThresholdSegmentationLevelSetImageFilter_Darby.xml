<?xml version="1.0"?>
<!DOCTYPE filter-gui SYSTEM "gui_filter.dtd">
<filter-gui name="Darby-ThresholdSegmentationLevelSetImageFilterGui">
  <parameters>
    <param name="lower_threshold">
      <gui>text-entry</gui>
      <default>210</default>
    </param>
    <param name="upper_threshold">
      <gui>text-entry</gui>
      <default>250</default>
    </param>
    <param name="curvature">
      <gui>scrollbar</gui>
      <min>0</min>
      <max>10</max>
      <default>1</default>
    </param>
    <param name="max_iterations">
      <gui>text-entry</gui>
      <default>1200</default>
    </param>
    <param name="max_rms_change">
      <gui>text-entry</gui>
      <default>0.02</default>
    </param>
    <param name="negative_features">
      <gui>checkbutton</gui>
      <default>1</default>
    </param>
    <param name="isovalue">
      <gui>text-entry</gui>
      <default>127.5</default>
    </param>
  </parameters>


</filter-gui>
