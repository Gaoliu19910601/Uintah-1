<!DOCTYPE component SYSTEM "../../../../../doc/Utilities/XML/component.dtd">
<?xml-stylesheet href="../../../../../doc/Utilities/XML/package-component.xsl"type="text/xsl"?>
<?cocoon-process type="xslt"?>
<component name="CannySegmentationLevelSetImageFilter" category="Filters">
  <overview>
    <authors>
    </authors>
    <summary>    
  Segments structures in images based on image features derived from pseudo-canny-edges.
    </summary>
  </overview>
  <implementation>
  </implementation>
  <io>
    <inputs lastportdynamic="no">  
      <port>
        <name>SeedImage</name>
        <datatype>Insight::ITKDatatype</datatype>
      </port>  
      <port>
        <name>FeatureImage</name>
        <datatype>Insight::ITKDatatype</datatype>
      </port>
    </inputs>
    <outputs>  
      <port>
        <name>OutputModel</name>
        <datatype>Insight::ITKDatatype</datatype>
      </port>  
      <port>
        <name>OutputFeatures</name>
        <datatype>Insight::ITKDatatype</datatype>
      </port>
    </outputs>
  </io>
</component>
