# SCI Network 1.0

set DATADIR .
if [info exists env(INSIGHT_DATA)] {
   set DATADIR $env(INSIGHT_DATA)
}

::netedit dontschedule

set m0 [addModuleAtPosition "Insight" "DataIO" "ImageReaderFloat2D" 10 10]
set m1 [addModuleAtPosition "Insight" "Filters" "GradientAnisotropicDiffusionImageFilter" 10 90]
set m2 [addModuleAtPosition "Insight" "Filters" "GradientMagnitudeImageFilter" 10 170]
set m3 [addModuleAtPosition "Insight" "Filters" "WatershedImageFilter" 10 250]
set m4 [addModuleAtPosition "Insight" "Filters" "UnaryFunctorImageFilter" 10 330]
set m5 [addModuleAtPosition "Insight" "Converters" "ImageToField" 10 410]
set m6 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 10 490]
set m7 [addModuleAtPosition "SCIRun" "Render" "Viewer" 10 570]

addConnection $m0 0 $m1 0
addConnection $m1 0 $m2 0
addConnection $m2 0 $m3 0
addConnection $m3 0 $m4 0
addConnection $m4 0 $m5 0
addConnection $m5 0 $m6 0
addConnection $m6 0 $m7 0

set $m0-notes {}
set $m0-filename $DATADIR/BrainProtonDensitySlice.png
