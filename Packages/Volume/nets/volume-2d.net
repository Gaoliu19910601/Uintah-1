set $m5-ViewWindow_0-view-lookat-z {0.0}
set $m5-ViewWindow_0-view-up-x {-0.319474923982}
set $m5-ViewWindow_0-view-up-y {-0.697767518554}
set $m5-ViewWindow_0-view-up-z {0.641136696031}
set $m5-ViewWindow_0-view-fov {20.0}
set $m5-ViewWindow_0-global-light0 {1}
