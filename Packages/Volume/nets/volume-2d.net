# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {65 42 696 498}
set creationDate {Thu Jul 08 2004}
set creationTime {19:45:57}
set runDate {}
set runTime {}
set notes {}

# Create a Teem->DataIO->NrrdReader Module
set m1 [addModuleAtPosition "Teem" "DataIO" "NrrdReader" 531 91]

# Create a Volume->Visualization->VolumeVisualizer Module
set m2 [addModuleAtPosition "Volume" "Visualization" "VolumeVisualizer" 174 345]

# Create a Teem->DataIO->NrrdReader Module
set m3 [addModuleAtPosition "Teem" "DataIO" "NrrdReader" 65 43]

# Create a Volume->Visualization->NrrdTextureBuilder Module
set m4 [addModuleAtPosition "Volume" "Visualization" "NrrdTextureBuilder" 65 170]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 174 454]

# Create a Teem->DataIO->NrrdReader Module
set m6 [addModuleAtPosition "Teem" "DataIO" "NrrdReader" 270 42]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 286 171]

# Create a Volume->Visualization->EditTransferFunc2 Module
set m8 [addModuleAtPosition "Volume" "Visualization" "EditTransferFunc2" 513 231]

# Create the Connections between Modules
set c1 [addConnection $m3 0 $m4 0]
set c2 [addConnection $m4 0 $m2 0]
set c3 [addConnection $m2 0 $m5 0]
set c4 [addConnection $m1 0 $m8 1]
set c5 [addConnection $m6 0 $m4 1]
set c6 [addConnection $m8 0 $m2 2]

# Set GUI variables for the Teem->DataIO->NrrdReader Module
set $m1-filename "$DATADIR/volume/tooth-vg-histo.pnm"

# Set GUI variables for the Volume->Visualization->VolumeVisualizer Module
set $m2-shine {30}

# Set GUI variables for the Teem->DataIO->NrrdReader Module
set $m3-filename "$DATADIR/volume/tooth-nv-uc.nrrd"

$m5 addViewer
set $m5-ViewWindow_0-geometry 654x648+909+491

# Set GUI variables for the SCIRun->Render->Viewer Module
set $m5-ViewWindow_0-pos {}
set $m5-ViewWindow_0-caxes {0}
