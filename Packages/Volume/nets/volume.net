# Ask SCIRun to tell us where the data is
#init_DATADIR_and_DATASET

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {10 9 406 368}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 10 10]

# Create a Volume->Visualization->TextureBuilder Module
set m2 [addModuleAtPosition "Volume" "Visualization" "TextureBuilder" 10 88]

# Create a Volume->Visualization->VolumeVisualizer Module
set m3 [addModuleAtPosition "Volume" "Visualization" "VolumeVisualizer" 79 234]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 211 9]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 47 324]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m2 0 $m3 0]
set c3 [addConnection $m3 0 $m5 0]
set c4 [addConnection $m4 0 $m3 1]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename {/home/ikits/SCIRunData/1.20.2/aneurysm/aneurysm-mra.lvs.fld}

# Setup GUI for the Volume->Visualization->TextureBuilder Module
setGlobal $m2-max {251}
setGlobal $m2-card_mem {8}

# Setup GUI for the Volume->Visualization->VolumeVisualizer Module
setGlobal $m3-num_slices {250}
setGlobal $m3-alpha_scale {-0.796}

# Setup GUI for the SCIRun->Visualization->GenStandardColorMaps Module
setGlobal $m4-positionList {{0 40} {13 40} {19 21} {173 21} {193 5} {222 6} {241 20} {277 21} {285 40} {295 40}}
setGlobal $m4-nodeList {257 258 259 791 2246 1057 526 260 261 262}
