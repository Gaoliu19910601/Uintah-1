SRCTOP   = @top_srcdir@
SRCDIR   = @srcdir@
OBJTOP   = ../..

INCLUDES = -I$(SRCTOP) -I$(SRCTOP)/DaveW/ThirdParty/Nrrd/include/ -I$(OBJTOP)
LIBRARIES = -L$(OBJTOP)/SCICore -lSCICore

all:: prog-SurfToJAS

prog-CVRTItoMesh:
	@$(MAKE) --no-print-directory SRCS=CVRTItoMesh.cc PROGRAM=CVRTItoMesh CVRTItoMesh

prog-JAStoSurf:
	@$(MAKE) --no-print-directory SRCS=JAStoSurf.cc PROGRAM=JAStoSurf JAStoSurf

prog-SFRGfile:
	@$(MAKE) --no-print-directory SRCS=SFRGfile.cc PROGRAM=SFRGfile SFRGfile

prog-SurfToJAS:
	@$(MAKE) --no-print-directory SRCS=SurfToJAS.cc PROGRAM=SurfToJAS SurfToJAS

_cleanHere::
	rm -f *.o *.d

include $(SRCTOP)/scripts/program.mk
