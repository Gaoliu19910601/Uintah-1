# SCI Network 1.0

::netedit dontschedule

set m0 [addModuleAtPosition "SCIRun" "Fields" "Centroids" 151 100]
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 64 12]
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 61 373]
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 61 464]
set m4 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 234 284]
set m5 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 309 123]
set m6 [addModuleAtPosition "SCIRun" "Fields" "DirectInterpolate" 61 189]

addConnection $m1 0 $m0 0
addConnection $m2 0 $m3 0
addConnection $m5 0 $m4 0
addConnection $m4 0 $m2 1
addConnection $m0 0 $m6 1
addConnection $m6 0 $m4 1
addConnection $m6 0 $m2 0
addConnection $m1 0 $m6 0

set $m0-notes {}
set $m0-show_status {1}
