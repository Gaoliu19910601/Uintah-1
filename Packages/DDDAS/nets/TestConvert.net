# SCI Network 1.0

::netedit dontschedule

global notes
set notes ""

set m0 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 61 478]
set m1 [addModuleAtPosition "SCIRun" "Render" "Viewer" 61 572]
set m2 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 198 398]
set m3 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 295 265]
set m4 [addModuleAtPosition "SCIRun" "Fields" "DirectInterpolate" 61 260]
set m5 [addModuleAtPosition "DDDAS" "DataIO" "Reader" 61 9]
set m6 [addModuleAtPosition "SCIRun" "Fields" "Unstructure" 61 95]
set m7 [addModuleAtPosition "SCIRun" "Fields" "HexToTet" 61 175]

addConnection $m0 0 $m1 0
addConnection $m3 0 $m2 0
addConnection $m2 0 $m0 1
addConnection $m4 0 $m2 1
addConnection $m4 0 $m0 0
addConnection $m5 0 $m6 0
addConnection $m6 0 $m7 0
addConnection $m7 0 $m4 0

set $m0-notes {}
set $m0-show_status {1}
set $m0-nodes-on {1}
set $m0-nodes-as-disks {0}
set $m0-edges-on {0}
