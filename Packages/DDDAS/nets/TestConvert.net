set $m0-use-transparency {0}
set $m0-vectors-on {0}
set $m0-normalize-vectors {}
set $m0-has_vector_data {0}
set $m0-bidirectional {0}
set $m0-arrow-heads-on {1}
set $m0-tensors-on {0}
set $m0-has_tensor_data {0}
set $m0-text-on {0}
set $m0-text-use-default-color {1}
set $m0-text-color-r {1.0}
set $m0-text-color-g {1.0}
set $m0-text-color-b {1.0}
set $m0-text-backface-cull {0}
set $m0-text-fontsize {1}
set $m0-text-precision {2}
set $m0-text-render_locations {0}
set $m0-text-show-data {1}
set $m0-text-show-nodes {0}
set $m0-text-show-edges {0}
set $m0-text-show-faces {0}
