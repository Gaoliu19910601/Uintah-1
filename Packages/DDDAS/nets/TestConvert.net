set $m0-active_tab {Nodes}
set $m0-node_scale {0.0100}
set $m0-edge_scale {0.0150}
set $m0-vectors_scale {0.30}
set $m0-tensors_scale {0.30}
set $m0-show_progress {}
set $m0-interactive_mode {Interactive}
set $m0-node-resolution {6}
set $m0-edge-resolution {6}
set $m0-data-resolution {6}
set $m1-notes {}
set $m1-show_status {1}
set $m2-notes {}
