<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE component SYSTEM "../../../../../doc/Utilities/XML/component.dtd">
<?xml-stylesheet href="../../../../../doc/package-component.xsl" type="text/xsl"?>
<?cocoon-process type="xslt"?>

<component name="RawToDenseMatrix" category="DataIO">
  <overview>
    <authors>
      <author>Martin Cole</author>
    </authors>
    <summary>Read CVRTI potentials, and output as a DenseMatrix</summary>
    <description>
      <p>Input data from CVRTI formatted .pots file.  The Output is a
DenseMatrix arranged with each lead in the rows, each column is a time
instance. The UI sets the start, end and units for the time steps.</p>
    </description>
  </overview>
  <implementation>
  </implementation>
  <io>
    <inputs lastportdynamic="no">
    </inputs>
    <outputs>
      <port>
        <name>DenseMatrix</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
    </outputs>
  </io>
  <gui>
    <parameter>
      <widget>Label</widget>
      <label>Autogenerated GUI explanation</label>
    </parameter>
  </gui>
  <testing>
  </testing>
</component>

