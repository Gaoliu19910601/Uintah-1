<?xml version='1.0' encoding='ISO-8859-1' ?>
<Uintah_Include>

  <Uintah_specification> 
    <ElectroChem           spec="OPTIONAL NO_DATA">
      <delt                spec="REQUIRED DOUBLE 'positive'" />
    </ElectroChem>
  </Uintah_specification>

  <MaterialProperties>
    <ElectroChem         spec="OPTIONAL NO_DATA">
      <material          spec="MULTIPLE NO_DATA" attribute1="name OPTIONAL STRING" >
        <diffusion_coeff spec="OPTIONAL DOUBLE"/>
        <mobility        spec="OPTIONAL DOUBLE"/>
        <geom_object     spec="MULTIPLE" />
      </material>
      <material_interface spec="REQUIRED NO_DATA"
                          attribute1="type REQUIRED STRING 'null'"> 
        <materials        spec="OPTIONAL MULTIPLE_INTEGERS" />
      </material_interface>
    </ElectroChem>
  </MaterialProperties>

  <AMR>
    <ElectroChem   spec="OPTIONAL NO_DATA" >
    </ElectroChem>
  </AMR>
  
</Uintah_Include>
