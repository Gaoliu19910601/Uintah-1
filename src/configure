    if test ! -e $_sci_lib/lib$i.so && test ! -e $_sci_lib/lib$i.dylib && test ! -e $_sci_lib/lib$i.a && test ! -e $_sci_lib/$i.lib; then
     as_fn_error $? "Specifically requested GLEW library file '$_sci_lib/lib$i' (.so .dylib or .a) was not found" "$LINENO" 5
    fi
