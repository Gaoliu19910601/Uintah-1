eval HAVE_ITCL_H="no"
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: not found" >&5
$as_echo "not found" >&6; }
if test "$_sci_optional" != "optional"; then

## SCI_MSG_ERROR
if test -z "$debugging"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:
