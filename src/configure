     as_fn_error $? "Specifically requested GL include file '$_sci_inc/$i' was not found" "$LINENO" 5
    fi
  done
  # Make sure the exact libraries were found
  for i in "" glu32 opengl32 gdi32 user32 advapi32 $GLU_OTHERS; do
    if test -z "$i"; then
       continue
    fi
    has_minus=`echo $i | sed 's/-.*//'`
    if test -z "$has_minus"; then
       i=`echo $i | sed 's/-l//g'`
    fi
    if test ! -e $_sci_lib/lib$i.so && test ! -e $_sci_lib/lib$i.dylib && test ! -e $_sci_lib/lib$i.a && test ! -e $_sci_lib/$i.lib; then
     as_fn_error $? "Specifically requested GL library file '$_sci_lib/lib$i' (.so .dylib or .a) was not found" "$LINENO" 5
    fi
  done
fi


