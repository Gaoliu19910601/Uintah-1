##      LAPACK_LIB_DIR        => lib path
##      LAPACK_LIB_DIR_FLAG   => all the -L's
##      LAPACK_LIB_FLAG       => all the -l's
##      HAVE_LAPACK           => yes or no
##      INC_LAPACK_H          => all the -I's
##      HAVE_LAPACK_H         => yes or no

