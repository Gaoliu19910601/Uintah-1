for _dir in "" $_sci_lib_path; do
  if test -n "$_dir" && test "$_dir" != "-L/usr/lib" && test "$_dir" != "-L$SCI_THIRDPARTY_LIB_DIR"; then
    _final_dirs="$_final_dirs $_dir"
  fi
