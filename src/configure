    if test "$value" = ""; then
      continue
    fi
    if test $__sci_first_time = "true"; then
      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
  if test $__sci_pass != "true"; then
       as_fn_error $? "For specific SCI-TRY-LINK test for ITK only one library path may be specified for arg 6 (you had: $_sci_lib).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." "$LINENO" 5
  fi
  # and only one include path:
  ### Determine if there is only one item in $_x_inc_dir $_sci_inc
