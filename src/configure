              { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Only -L options are allowed in arg 6 of UNP check (disregarding $path)." >&5
$as_echo "$as_me: WARNING: Only -L options are allowed in arg 6 of UNP check (disregarding $path)." >&2;}
            fi
            continue
         fi
