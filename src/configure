  ### Determine if there is only one item in $_sci_lib (I don't know of a better way to do this.)
  __sci_pass=false
  __sci_first_time=true

  # Must have the "" for the SGI sh.
  for value in "" $_sci_lib; do
    if test "$value" = ""; then
      continue
    fi
    if test $__sci_first_time = "true"; then
      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
  if test $__sci_pass != "true"; then
       as_fn_error $? "For specific SCI-TRY-LINK test for GLUI only one library path may be specified for arg 6 (you had: $_sci_lib).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." "$LINENO" 5
  fi
  # and only one include path:
  ### Determine if there is only one item in $_sci_inc
  __sci_pass=false
  __sci_first_time=true
  for value in "" $_sci_inc; do
    if test "$value" = ""; then
      continue
    fi
    if test $__sci_first_time = "true"; then
      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
  if test -n "$_sci_inc" && test $__sci_pass != "true"; then
       as_fn_error $? "For specific SCI-TRY-LINK test for GLUI only one include path may be specified for arg 4 (you had: $_sci_inc).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." "$LINENO" 5
  fi
fi

