    if test ! -e $_sci_inc /usr/include/petsc/$i; then
     as_fn_error $? "Specifically requested PETSC include file '$_sci_inc /usr/include/petsc/$i' was not found" "$LINENO" 5
    fi
  done
  # Make sure the exact libraries were found
  for i in "" petsc petscvec petscmat petscdm petsccontrib petscsnes petscts petscsles; do
    if test -z "$i"; then
       continue
    fi
    has_minus=`echo $i | sed 's/-.*//'`
    if test -z "$has_minus"; then
       i=`echo $i | sed 's/-l//g'`
    fi
    if test ! -e $_sci_lib_dir/lib$i.so && test ! -e $_sci_lib_dir/lib$i.dylib && test ! -e $_sci_lib_dir/lib$i.a && test ! -e $_sci_lib_dir/$i.lib; then
     as_fn_error $? "Specifically requested PETSC library file '$_sci_lib_dir/lib$i' (.so .dylib or .a) was not found" "$LINENO" 5
    fi
  done
