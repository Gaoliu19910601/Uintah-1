            # Has some other -?.
            if test "$debugging" = "yes"; then
              echo
              { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Only -l options are allowed in arg 5 of CAMAL check (disregarding $lib)." >&5
$as_echo "$as_me: WARNING: Only -l options are allowed in arg 5 of CAMAL check (disregarding $lib)." >&2;}
            fi
            continue
         fi
      fi

      the_lib=`echo $lib | grep "^\-l"`
      if test -z "$the_lib" && test "$found_framework" = "no"; then
         # If the lib arg does not have -l on it, then add -l.
