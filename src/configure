$as_echo "$as_me: WARNING: If the error message is something like:" >&2;}
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: \`tenEigensolve_f' undeclared then you most likely" >&5
$as_echo "$as_me: WARNING: \`tenEigensolve_f' undeclared then you most likely" >&2;}
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: have too old of a thirdparty." >&5
$as_echo "$as_me: WARNING: have too old of a thirdparty." >&2;}
    as_fn_error $? "One or more of the TEEM components is missing." "$LINENO" 5
  fi
fi


