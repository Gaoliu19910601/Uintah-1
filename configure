  *-irix*)

    ##  SCI_CHECK_OS_VERSION

    _SCI_CORRECT_='echo -n ""'
    _SCI_NOTCORRECT_='echo -n ""'
    _SCI_VER_1_="0"
    _SCI_VER_2_="6.5"
