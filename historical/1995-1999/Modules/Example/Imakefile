
#include <config/sci.cf>

OBJS = GeomPortTest.o
SRCS = GeomPortTest.cc

LIBNAME = Example

LOCALINCLUDES = $(OPENGLINCS)
SO_LIBS =

SO_LDFLAGS = -L$(TOP)/lib -lDatatypes -lDataflow \
	-lGeom -lClasslib -lMultitask $(OPENGLLIBS) \
	-lMalloc -lWidgets -lGeometry \
	-lm SCI_CCLibs

SCIDependTarget($(SRCS))
SCIObjLibraryTarget($(LIBNAME),$(OBJS),$(SO_LIBS),$(SO_LDFLAGS))

