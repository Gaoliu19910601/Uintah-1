
#include <config/sci.cf>

OBJS = 	rMeshReader.o    \
	rMeshWriter.o  

SRCS = 	rMeshReader.cc    \
	rMeshWriter.cc  

TCLS =  rMeshReader.tcl \
	rMeshWriter.tcl

LIBNAME = Remote

SO_LIBS =
SO_LDFLAGS = -L$(TOP)/lib -lWidgets -lDatatypes -lDataflow -lGeom  \
	-lGeometry -lTCL \
	-lClasslib -lMultitask -lMalloc -lm SCI_CCLibs

depend:: $(SRCS)

clean::
	$(RM) $(OBJS) 

SCIDependTarget($(SRCS))
SCIObjLibraryTarget($(LIBNAME), $(OBJS),$(SO_LIBS),$(SO_LDFLAGS))
