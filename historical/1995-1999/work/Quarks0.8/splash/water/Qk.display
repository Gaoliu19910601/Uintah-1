#
# resources file for Quarks display windows
#

width:   68     # width of window in number of chars (def 68)
height:  11     # height of window in number of lines (def 11)
xdisp:   350    # horizontal displacement between windows in pixels (def 350)
ydisp:   170    # vertical displacement between windows in pixels (def 170) 
numcol1: 5      # number of windows in the first column   (def 5)
font:    7x13   # font to use for the text in the windows (def 7x13)


