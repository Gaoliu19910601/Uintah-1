<Uintah_specification>

  <Meta>
    <title>Momentum Test with Taylor Vortex MMS</title>
  </Meta>

  <SimulationComponent type="wasatch"/>

  <Wasatch>

    <!-- Set the initial condition for non-staggered scalar -->
    <!-- Set the initial condition expression for VelX -->
    <TaylorVortexMMS type="XVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="x-mom" state="STATE_N" />
      <VelocityX amplitude="1.0" viscosity="0.0">
        <XCoordinate> <NameTag name="XXVOL" state="STATE_NONE"/> </XCoordinate>
        <YCoordinate> <NameTag name="YXVOL" state="STATE_NONE"/> </YCoordinate>
      </VelocityX>
    </TaylorVortexMMS>

    <!-- Set the initial condition for VelY -->
    <TaylorVortexMMS type="YVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="y-mom" state="STATE_N" />
      <VelocityY amplitude="1.0" viscosity="0.0" >
        <XCoordinate> <NameTag name="XYVOL" state="STATE_NONE"/> </XCoordinate>
        <YCoordinate> <NameTag name="YYVOL" state="STATE_NONE"/> </YCoordinate>
      </VelocityY>
    </TaylorVortexMMS>

    <!-- BasicExpression type="XVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="x-mom" state="STATE_N" />
      <SineFunction amplitude="1.0" frequency="1.0" offset="0.0">
        <NameTag name="XXVOL" state="STATE_NONE" />
      </SineFunction>
    </BasicExpression>
    
    <BasicExpression type="YVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="y-mom" state="STATE_N" />
      <SineFunction amplitude="0.0" frequency="1.0" offset="0.0">
        <NameTag name="XYVOL" state="STATE_NONE" />
      </SineFunction>
    </BasicExpression-->


    
    <!-- Set the initial condition for non-staggered scalar -->
    <BasicExpression type="ZVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="z-mom" state="STATE_N" />
      <Constant>0.0</Constant>
    </BasicExpression>
    
    <!-- BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="pressure" state="STATE_N" />
      <Constant>1.0</Constant>
    </BasicExpression>    
    
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="density" state="STATE_N" />
      <Constant>1.0</Constant>
    </BasicExpression>
    
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="viscosity" state="STATE_N" />
      <Constant>0.001</Constant>
    </BasicExpression>

    <BasicExpression type="XVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="tau_xx" state="STATE_N" />
      <Constant>1</Constant>
    </BasicExpression>
    <BasicExpression type="YVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="tau_yy" state="STATE_N" />
      <Constant>1</Constant>
    </BasicExpression>
    <BasicExpression type="ZVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="tau_zz" state="STATE_N" />
      <Constant>1</Constant>
    </BasicExpression>

    <BasicExpression type="XVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="u" state="STATE_N" />
      <Constant>1</Constant>
    </BasicExpression>
    <BasicExpression type="YVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="v" state="STATE_N" />
      <Constant>1</Constant>
    </BasicExpression>
    <BasicExpression type="ZVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="w" state="STATE_N" />
      <Constant>1</Constant>
    </BasicExpression -->
    
    <!--BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="body-force" state="STATE_NONE" />
      <GaussianFunction amplitude="0.2" deviation="4.0" mean="10.0" baseline="0.1">
        <NameTag name="XSVOL" state="STATE_NONE" />
      </GaussianFunction>
    </BasicExpression>
    
    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="density-acceleration" state="STATE_NONE" />
      <GaussianFunction amplitude="0.2" deviation="4.0" mean="10.0" baseline="0.1">
        <NameTag name="XSVOL" state="STATE_NONE" />
      </GaussianFunction>
    </BasicExpression-->

    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="viscosity" state="STATE_N" />
      <Constant>0.001</Constant>
    </BasicExpression>

    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="density" state="STATE_N" />
      <Constant>1.0</Constant>
    </BasicExpression>

    <!-- Momentum transport equations -->
    <MomentumEquations>
      <X-Velocity>u</X-Velocity>
      <X-Momentum>x-mom</X-Momentum>
      <Y-Velocity>v</Y-Velocity>
      <Y-Momentum>y-mom</Y-Momentum>
      <!--Z-Velocity>w</Z-Velocity>
      <Z-Momentum>z-mom</Z-Momentum-->            
      <Viscosity>
        <NameTag name="viscosity" state="STATE_N" />
      </Viscosity>
      
      <Pressure>
        <Parameters>

            <!-- CGSolver options -->
              <norm>     LInfinity  </norm>
              <criteria> Absolute   </criteria>

            <!-- Hypre options -->
            <solver>         cg      </solver>
            <preconditioner> pfmg    </preconditioner>
            <tolerance>      1.e-8  </tolerance>
            <maxiterations>  1000    </maxiterations>
            <npre>           1       </npre>
            <npost>          1       </npost>
            <skip>           0       </skip>
            <jump>           0       </jump>
           </Parameters>

      </Pressure>
    </MomentumEquations>


  </Wasatch>


  <Time>
    <maxTime>1.0</maxTime>
    <initTime>0.0</initTime>
    <delt_min>0.005</delt_min>
    <delt_max>0.005</delt_max>
    <timestep_multiplier>1.0</timestep_multiplier>
  </Time>


  <DataArchiver>
    <filebase>momentum-test-mms.uda</filebase>
    <outputTimestepInterval>1</outputTimestepInterval>
    <outputInitTimestep/>
    <checkpoint cycle = "2" interval = "0.2"/>
    <compression>gzip</compression>
    <save label="x-mom"/>
    <save label="y-mom"/>
        <save label="u"/>
            <save label="v"/>
    <!-- save label="z-mom"/ -->
    <save label="pressure"/>
    <save label="x-mom_convFlux_x"/>
    <save label="x-mom_convFlux_y"/>
    <!--save label="x-mom_convFlux_z"/-->
    <save label="y-mom_convFlux_x"/>
    <save label="y-mom_convFlux_y"/>
    <!--save label="y-mom_convFlux_z"/-->
    <!--save label="z-mom_convFlux_x"/>
    <save label="z-mom_convFlux_y"/>
    <save label="z-mom_convFlux_z"/-->

    <save label="dilatation"/>
    <save label="density"/>
    <save label="tau_xx"/>
    <save label="tau_xy"/>
    <!--save label="tau_xz"/-->
    <save label="tau_yy"/>
    <save label="tau_yx"/>
    <!--save label="tau_yz"/-->
    <!-- save label="tau_zz"/>
    <save label="tau_zx"/>
    <save label="tau_zy"/-->
    <save label="x-mom_rhs_partial"/>
    <save label="y-mom_rhs_partial"/>
    <save label="x-mom_rhs_full"/>
    <save label="y-mom_rhs_full"/>

  </DataArchiver> 

  <Grid>
  
    <BoundaryConditions>
      
      <!-- BOUNDARY CONDITIONS FOR X-MOMENTUM -->
      <Face side="x-">
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 10.0 </value>
        </BCType>
      </Face>
      <Face side="x+">
        <BCType id="all" var="Neumann" label="x-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="y-">
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="y+">
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="z-">
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="z+">
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      
      <!-- BOUNDARY CONDITIONS FOR Y-MOM -->
      <Face side="x-">
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="x+">
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="y-">
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="y+">
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="z-">
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="z+">
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>

      <!-- BOUNDARY CONDITIONS FOR Z-MOM -->
      <Face side="x-">
        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="x+">
        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="y-">
        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="y+">
        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="z-">
        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side="z+">
        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
      </Face>

    </BoundaryConditions>
    <!--0.062831853071796-->
    <!--0.031415926535898-->
    <Level>
      <Box label="1">
        <!--lower>[-3.078760800517997,-3.078760800517997,0]</lower>
        <upper>[3.204424506661589,3.204424506661589,0.000001]</upper-->
        <lower>[0,0,0]</lower>
        <upper>[6.283185307179586,6.283185307179586,2]</upper>

        <resolution>[100,100,4]</resolution>
        <patches>[1,1,1]</patches>
      </Box>
      <periodic>[1,1,1]</periodic>
    </Level>
  </Grid>

</Uintah_specification>
