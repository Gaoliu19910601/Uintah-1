              <resolution>  [40,40,1]         </resolution>
              <patches>     [2,2,1]           </patches>
              <extraCells>  [0,0,0]           </extraCells>
           </Box>
       </Level>
       <Level>
           <Box label = "1">
              <lower>       [0.5,0.5,-0.05]   </lower>
