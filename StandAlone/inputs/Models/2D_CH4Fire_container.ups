<?xml version="1.0" encoding="iso-8859-1"?>
<!-- @version: -->
<Uintah_specification>

   <Meta>
       <title/>
   </Meta>
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            1.0         </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           1.0e-15     </delt_min>
       <delt_max>           1.0e-4      </delt_max>
       <delt_init>          1.0e-7      </delt_init>
       <max_delt_increase>  0.15        </max_delt_increase>
       <timestep_multiplier>0.5         </timestep_multiplier>
   </Time>
   
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <Face side = "x-">
         <BCType id = "all" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "scalar-f"     var = "Dirichlet">
                              <value> 0.0</value>
        </BCType>
        <BCType id = "all" label = "cumulativeEnergyReleased" var = "Dirichlet">
                              <value> 0.0</value>
        </BCType>
      </Face>
      <!--__________________________________-->
      
      <Face circle = "y-" origin = "0.0 -0.1 0.0" radius = ".05">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "2" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,10.,0.] </value>
        </BCType>
        <BCType id = "2" label = "Temperature"  var = "Dirichlet">
                              <value> 300.0  </value>
        </BCType>
        <BCType id = "2" label = "Density"      var = "Dirichlet">
                              <value> 0.6560618010  </value>
        </BCType>
        <BCType id = "2" label = "scalar-f"     var = "Dirichlet">
                              <value> 1.0</value>
        </BCType>
        <BCType id = "2" label = "cumulativeEnergyReleased" var = "Dirichlet">
                              <value> 0.0</value>
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "scalar-f"     var = "Dirichlet">
                              <value> 0.0</value>
        </BCType>
        <BCType id = "all" label = "cumulativeEnergyReleased" var = "Dirichlet">
                              <value> 0.0</value>
        </BCType>
      </Face>   
      <!--__________________________________-->               
      <Face side = "y+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>

        <BCType id = "all" label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "scalar-f"     var = "Neumann">
                              <value> 0.0</value>
        </BCType>
        <BCType id = "all" label = "cumulativeEnergyReleased" var = "Neumann">
                              <value> 0.0</value>
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
      </Face>                  
      <Face side = "z+">
        <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>        [0.0,-0.1,0.0]     </lower>
              <upper>        [0.1, 0.2,0.00125]     </upper>
              <extraCells>   [1,1,1]            </extraCells>
              <patches>      [3,4,1]            </patches>
              <resolution>   [80,240,1]        </resolution>
           </Box>
           
       </Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>2D_CH4Fire_container.uda</filebase>
      <outputInterval> 0.0005  </outputInterval>
      <!--<outputTimestepInterval> 1 </outputTimestepInterval>-->
      <save label="vol_frac_CC"/>
      <save label="press_CC"/>
      <save label="temp_CC"/>
      <save label="rho_CC"/>
      <save label="sp_vol_CC"/>
      <save label="delP_Dilatate"/>
      <save label="vel_CC"/>
      <save label="scalar-f"/> 
      <save label= "cumulativeEnergyReleased"/>
      <save label= "modelMass_src"/>
      <save label= "modelMom_src"/>
      <save label= "modelEng_src"/>
      
      <save label="p.x"/>
      <save label="p.volume"/>
     <!-- <save label="p.mass"/>
      <save label="p.velocity"/>
      <save label="p.temperature"/>
      <save label="p.stress"/> 
      <save label="g.mass"/>
      <save label="g.temperature"/>  -->
      <checkpoint cycle = "4" timestepInterval = "200"/>
   </DataArchiver>

   <Debug>
     <dbg_timeStart>       3.0        </dbg_timeStart>
     <dbg_timeStop>        3.0        </dbg_timeStop>
     <dbg_outputInterval>  1e-9       </dbg_outputInterval>
     <dbg_BeginIndex>      [-1,-1,-1] </dbg_BeginIndex>
     <dbg_EndIndex>        [2, 2, 2]  </dbg_EndIndex>
     <dbg_SigFigs>         5          </dbg_SigFigs>
     <dbg_Matls>        [0,1,2]       </dbg_Matls>
     <debug label = "switchDebugLagrangianTransportedVars"/>
     <debug label = "switchDebug_advance_advect"/> 
   </Debug>
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
       <cfl>0.5</cfl>
       <ICE>
        <max_iteration_equilibration> 1000 </max_iteration_equilibration>
        <advection type = "FirstOrder" />
        <solution technique = "EqForm" />

        <TimeStepControl>
          <Scheme_for_delT_calc> conservative </Scheme_for_delT_calc>  
          <knob_for_speedSound>  0.000        </knob_for_speedSound>   
        </TimeStepControl>

        <ImplicitSolver>
           <max_outer_iterations>      20     </max_outer_iterations>
           <outer_iteration_tolerance> 1e-6   </outer_iteration_tolerance>
           <iters_before_timestep_restart> 10 </iters_before_timestep_restart>
            <Parameters>
               <tolerance>1.e-6      </tolerance>
               <norm>     LInfinity  </norm>
               <criteria> Absolute   </criteria>
             <!-- Hypre options -->
             <solver>         cg     </solver>
             <preconditioner> pfmg   </preconditioner>
             <maxiterations>  200    </maxiterations>
             <npre>           1      </npre>  
             <npost>          1      </npost> 
             <skip>           1      </skip>  
             <jump>           0      </jump>  
            </Parameters>
        </ImplicitSolver>
      </ICE>        
    </CFD>
    <MPM>
        <time_integrator>explicit</time_integrator>
        <nodes8or27>27</nodes8or27>
        <minimum_particle_mass>          3.e-15     </minimum_particle_mass>
        <maximum_particle_velocity>      5.e3       </maximum_particle_velocity>
        <artificial_damping_coeff>       0.0        </artificial_damping_coeff>
        <artificial_viscosity>           true       </artificial_viscosity>
        <artificial_viscosity_coeff1>    0.07       </artificial_viscosity_coeff1>
        <artificial_viscosity_coeff2>    1.6        </artificial_viscosity_coeff2>
        <turn_on_adiabatic_heating>      false      </turn_on_adiabatic_heating>
        <accumulate_strain_energy>       false      </accumulate_strain_energy>
        <use_load_curves>                false      </use_load_curves>
        <create_new_particles>           false      </create_new_particles>
        <erosion algorithm = "none"/>
    </MPM>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,-9.81,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>
    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <MPM>
        <material name = "Steel Ring">
        <include href="inputs/MPM/MaterialData/MaterialConst4340Steel.xml"/>
        <constitutive_model type="elastic_plastic">
          <useModifiedEOS>                true  </useModifiedEOS>
          <tolerance>                  1.0e-15  </tolerance>
          <evolve_porosity>               true  </evolve_porosity>
          <evolve_damage>                 true  </evolve_damage>
          <compute_specific_heat>         true  </compute_specific_heat>
          <do_melting>                    true  </do_melting>
          <check_TEPLA_failure_criterion> true  </check_TEPLA_failure_criterion>
       <include href="inputs/MPM/MaterialData/IsotropicElastic4340Steel.xml"/>
       <include href="inputs/MPM/MaterialData/MieGruneisenEOS4340Steel.xml"/>
       <include href="inputs/MPM/MaterialData/JohnsonCookPlastic4340Steel.xml"/>
       <include href="inputs/MPM/MaterialData/GursonYieldSteel.xml"/>
       <include href="inputs/MPM/MaterialData/DruckerBeckerStabilityCheck.xml"/>
       <include href="inputs/MPM/MaterialData/JohnsonCookDamage4340Steel.xml"/>
          <initial_material_temperature>  294.0 </initial_material_temperature>
          <initial_mean_porosity>         0.005 </initial_mean_porosity>
          <initial_std_porosity>          0.001 </initial_std_porosity>
          <critical_porosity>             0.5   </critical_porosity>
          <frac_nucleation>               0.1   </frac_nucleation>
          <meanstrain_nucleation>         0.3   </meanstrain_nucleation>
          <stddevstrain_nucleation>       0.1   </stddevstrain_nucleation>
          <initial_porosity_distrib>      gauss </initial_porosity_distrib>
          <initial_mean_scalar_damage>    0.01  </initial_mean_scalar_damage>
          <initial_std_scalar_damage>     0.005 </initial_std_scalar_damage>
          <critical_scalar_damage>        1.0   </critical_scalar_damage>
          <initial_scalar_damage_distrib> gauss </initial_scalar_damage_distrib>
        </constitutive_model>
                <geom_object>
                  <difference>
                    <cylinder label = "outer cylinder">
                     <bottom>           [0.0,0.0,-.05715]   </bottom>
                     <top>              [0.0,0.0, .05715]   </top>
                     <radius>           0.05715             </radius>
                   </cylinder>
                   <cylinder label = "inner cylinder">
                     <bottom>           [0.0,0.0,-.0508]   </bottom>
                     <top>              [0.0,0.0, .0508]   </top>
                     <radius>           0.0508             </radius>
                   </cylinder>
                  </difference>
                 <res>                 [2,2,2]         </res>
                 <velocity>            [0.0,0.0,0.0]   </velocity>
                 <temperature>        300.0            </temperature>
                </geom_object>
         </material>
         
         <material  name = "reactant">
                <density>   1840.0   </density>
                <constitutive_model type = "visco_scram">
                    <useModifiedEOS>     true       </useModifiedEOS>
                    <PR>0.3</PR>
                    <CrackParameterA>   1.e-3 </CrackParameterA>
                    <CrackPowerValue>   10.0  </CrackPowerValue>
                    <CrackMaxGrowthRate>300.0 </CrackMaxGrowthRate>
                    <StressIntensityF>  500.0 </StressIntensityF>
                    <CrackFriction>     0.35  </CrackFriction>
                    <InitialCrackRadius>3.e-5 </InitialCrackRadius>
                    <CrackGrowthRate>   12.   </CrackGrowthRate>
                    <G1>  3.5e8   </G1>
                    <G2>  1.739e8 </G2>
                    <G3>  5.212e8 </G3>
                    <G4>  9.085e8 </G4>
                    <G5>  6.875e8 </G5>
                    <RTau1> 0.0    </RTau1> 
                    <RTau2> 7.32e3 </RTau2> 
                    <RTau3> 7.32e4 </RTau3> 
                    <RTau4> 7.32e5 </RTau4> 
                    <RTau5> 2.e6   </RTau5> 
                    <Beta>  1.5e-4 </Beta>  
                    <Gamma> 1.0    </Gamma> 
                    <DCp_DTemperature>3.30773</DCp_DTemperature>
               </constitutive_model>
               <toughness>             10.e6            </toughness>
                <thermal_conductivity>  .452            </thermal_conductivity>
                <specific_heat>        996.0            </specific_heat>
                <geom_object>
                   <cylinder label = "inner cylinder">
                     <bottom>           [0.0,0.0,-.0508]   </bottom>
                     <top>              [0.0,0.0, .0508]   </top>
                     <radius>           0.0508             </radius>
                  </cylinder>
                  <res>                 [2,2,2]         </res>
                  <velocity>            [0.0,0.0,0.0]   </velocity>
                  <temperature>         300.0           </temperature>
                </geom_object>
         </material>

            <contact>
              <type>approach</type>
              <vel_fields>              [0,0,0]         </vel_fields>
              <mu> 0.0 </mu>
            </contact>
            <thermal_contact></thermal_contact>
      </MPM>
      <!--__________________________________-->
       <ICE>
         <material name = "product">       
           <EOS type = "ideal_gas"> </EOS>
           <isSurroundingMatl>          true            </isSurroundingMatl>
           <dynamic_viscosity>          1e-6            </dynamic_viscosity>
           <thermal_conductivity>       26.3e-3         </thermal_conductivity>
           <specific_heat>              716.0           </specific_heat>
           <speed_of_sound>             376             </speed_of_sound>
           <gamma>                      1.4             </gamma>           
           <geom_object>
                 <difference>
                  <box label="domain">
                      <min>           [-1.1,-1.1,-1.1]    </min>
                      <max>           [ 1.1, 1.1, 1.1]    </max>
                  </box>
                  <cylinder label = "container">
                     <bottom>       [0.0,0.0,-.05715]     </bottom>
                     <top>          [0.0,0.0, .05715]     </top>
                     <radius>       0.05715               </radius>
                   </cylinder>
                 </difference>
            <res>                      [2,2,2]       </res>
            <velocity>           [0.0,0.0,0.0]       </velocity>
            <temperature>        300.0               </temperature>
            <density>            1.1792946927374306  </density>
            <pressure>           101325.0            </pressure>
           </geom_object>
         </material>
      </ICE>       
       
      <exchange_properties> 
         <exchange_coefficients>
              <momentum>  [ 0, 1e15, 1e15]     </momentum>
              <heat>      [ 0, 1e10, 1e10]     </heat>
        </exchange_coefficients>
      </exchange_properties> 
    </MaterialProperties>
    <!--____________________________________________________________________-->
    <!--      M O D E L S                                                   -->
    <!--____________________________________________________________________-->

    <Models>
<!--
      <Model type="Simple_Burn">
        <fromMaterial> reactant     </fromMaterial>
        <toMaterial>  product       </toMaterial>
        <ThresholdTemp>       450.0 </ThresholdTemp>
        <ThresholdPressure> 50000.0 </ThresholdPressure>
        <Enthalpy>        2000000.0 </Enthalpy>
        <BurnCoeff>            75.3 </BurnCoeff>
        <refPressure>      101325.0 </refPressure>
      </Model>
 -->
       <Model type="AdiabaticTable">
        <table name="adiabatic" type="Arches">
	  <defaultValue name="heat loss">0.0</defaultValue>
          <derivedValue name="reference heat capacity Cv(j/kg-K)">[heat capacity Cv(j/kg-K)]</derivedValue>
          <derivedValue name="reference gamma">[gamma]</derivedValue>
          <constantValue name="reference Temp(K)">298</constantValue>
	  <filename>inputs/Models/ch4.rxn</filename>
	</table>
        <material>product</material>
        <scalar name="f">
          <constants>
        <diffusivity>0</diffusivity></constants>
          <geom_object>
            <difference>
             <box label="domain">
                 <min>           [-1.1,-1.1,-1.1]    </min>
                 <max>           [ 1.1, 1.1, 1.1]    </max>
             </box>
             <cylinder label = "container">
                <bottom>       [0.0,0.0,-.05715]     </bottom>
                <top>          [0.0,0.0, .05715]     </top>
                <radius>       0.05715               </radius>
              </cylinder>
            </difference>
            <scalar>  0.0 </scalar>
          </geom_object>
	</scalar>
      </Model>
     
    </Models>            
   
    <PhysicalBC>
       <MPM>
      </MPM>
    </PhysicalBC>
       
</Uintah_specification>
