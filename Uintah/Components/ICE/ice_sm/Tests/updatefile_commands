
set SRC = ~/Csafe/Uintah_cfd_code/Tests/Riemann
/*__________________________________
* Makefile
*___________________________________*/
cp Makefile Makefile_org; xdiff Makefile $SRC/Makefile
mv Makefile_tmp Makefile
/*__________________________________
* Overides
*___________________________________*/
cp overides.c overides.c_org; xdiff overides.c $SRC/overides.c
mv overides.c_tmp overides.c
/*__________________________________
* Parameters
*___________________________________*/
cp parameters.h parameters.h_org; xdiff parameters.h $SRC/parameters.h
mv parameters.h_tmp parameters.h
/*__________________________________
* Switches
*___________________________________*/
cp switches.h switches.h_org; xdiff switches.h $SRC/switches.h
mv switches.h_tmp switches.h
/*__________________________________
* if
*___________________________________*/
cp if if_org;  xdiff if $SRC/if
mv if_tmp if

/*__________________________________
* debugcode.i
*___________________________________*/
cp debugcode.i debugcode.i_org;  xdiff debugcode.i $SRC/debugcode.i
mv debugcode.i_tmp debugcode.i

